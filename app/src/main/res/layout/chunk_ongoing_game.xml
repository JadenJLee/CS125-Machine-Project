    android:layout_height="wrap_content"
    android:visibility="gone">

    <TextView
        android:id="@+id/OGemail"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:text="PlayerEmail" />

    <TextView
        android:id="@+id/OGteam"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:text="OGteam" />

    <TextView
        android:id="@+id/OGgamemode"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:text="OGgamemode" />

</LinearLayout>